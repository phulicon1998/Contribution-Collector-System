{
    "name": "frt",
    "version": "1.1.1",
    "private": true,
    "dependencies": {
        "axios": "^0.19.0",
        "bootstrap": "3.4.1",
        "chartist": "0.10.1",
        "history": "4.7.2",
        "js-yaml": "^3.13.1",
        "jwt-decode": "^2.2.0",
        "moment": "2.21.0",
        "node-sass": "^4.13.1",
        "node-sass-chokidar": "^1.3.5",
        "nouislider": "11.0.3",
        "npm-run-all": "4.1.2",
        "perfect-scrollbar": "1.3.0",
        "react": "^16.8.6",
        "react-big-calendar": "0.19.0",
        "react-bootstrap": "0.32.1",
        "react-bootstrap-sweetalert": "4.2.3",
        "react-bootstrap-switch": "15.5.3",
        "react-chartist": "0.13.1",
        "react-datetime": "2.14.0",
        "react-dom": "^16.8.6",
        "react-files": "^2.4.8",
        "react-google-maps": "9.4.5",
        "react-jvectormap": "0.0.2",
        "react-notification-system": "0.2.17",
        "react-redux": "^7.0.2",
        "react-router-dom": "^5.0.0",
        "react-scripts": "1.1.1",
        "react-select": "1.2.1",
        "react-stepzilla": "4.7.0",
        "react-syntax-highlighter": "7.0.2",
        "react-table": "6.8.0",
        "react-tagsinput": "3.19.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "webpack-dev-server": "^3.5.0",
        "wnumb": "1.1.0",
        "xlsx": "^0.14.3"
    },
    "proxy": "http://localhost:8080",
    "scripts": {
        "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "start-dev": "concurrently \"npm run watch-css\" \"set NODE_PATH=./src && react-scripts start\"",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "lint:check": "eslint . --ext=js,jsx;  exit 0",
        "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
        "build-package-css": "cp src/assets/sass/light-bootstrap-dashboard.css dist/light-bootstrap-dashboard.css",
        "build-package": "npm run build-package-css && babel src --out-dir dist"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {}
}
